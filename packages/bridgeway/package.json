{
  "name": "@vibrajs/bridgeway",
  "type": "module",
  "version": "0.0.0",
  "packageManager": "pnpm@8.1.0",
  "description": "",
  "author": "Julio Gim√©nez <julio.visionarte@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/juliogimenez/bridgeway-monorepo#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/juliogimenez/bridgeway-monorepo.git"
  },
  "bugs": "https://github.com/juliogimenez/bridgeway-monorepo/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "rimraf dist && pnpm run build:bundle -w",
    "build": "rimraf dist && pnpm run build:bundle",
    "build:bundle": "rollup --config rollup.config.ts --configPlugin typescript",
    "format": "prettier --write --cache --parser typescript \"src/**/*.ts\""
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^24.1.0",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@rollup/plugin-typescript": "^11.1.0",
    "rollup": "^3.20.2",
    "tslib": "^2.3.1"
  }
}
